{
  "name": "vue-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:zip": "vue-cli-service build && node zip.js",
    "analys": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "lint": "vue-cli-service lint",
    "deploy:dev": "npm run build && cross-env NODE_ENV=development node ./deploy",
    "deploy:prod": "npm run build && cross-env NODE_ENV=production node ./deploy"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compressing": "^1.5.1",
    "compression-webpack-plugin": "^4.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "js-cookie": "^2.2.1",
    "localforage": "^1.9.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.9.4",
    "vlf": "^1.1.0",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-parallel-uglify-plugin": "^1.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
